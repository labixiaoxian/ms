<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoxian.dao.UserDao">

    <select id="findById" parameterType="java.lang.Integer" resultType="com.xiaoxian.entity.User">
        SELECT
        id,name,password
        FROM
        user
        WHERE
        id = #{id}
    </select>

</mapper>